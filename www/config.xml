<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.myapp7ab618" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" defaultlocale="en-US">
  <name>Sincil</name>
  <description>Sincil Bank Regeneration Application</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Apache Cordova Team </author>
  <content src="index.html" />
  <access origin="*" />
  <vs:features />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.1" />
  <!-- Support for Cordova 5.0.0 plugin system -->
  
  <!-- Core plugins -->
  <plugin name="cordova-plugin-whitelist" />
  <plugin name="cordova-plugin-vibration" />
  <plugin name="cordova-plugin-geolocation" version="2.1.0" />
  
 <plugin name="cordova-plugin-camera" spec="2.4.1">
 	<param name="CAMERA_USAGE_DESCRIPTION" value="App would like to access the camera." />
 	<param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="App would like to access the library." />
</plugin>

<plugin name="cordova-plugin-media-capture" >
	<param name="CAMERA_USAGE_DESCRIPTION" value="We'd like to access your camera to let you take a photo"/>
	<param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="We'd like to access your photo library to let you pick a photo"/>
	<param name="MICROPHONE_USAGE_DESCRIPTION" value="We'd like to access your microphone to let you record an audio"/>
</plugin>

<feature name="Camera">
    <param name="android-package" value="org.apache.cordova.CameraLauncher" />
</feature>

<plugin spec="https://github.com/devgeeks/Canvas2ImagePlugin.git" source="git" />
<plugin spec="https://github.com/gitawego/cordova-screenshot.git" source="git" />
  
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  
  <platform name="android">
    <icon src="res/icons/ios/Sincilicon.png" density="ldpi" />
    <icon src="res/icons/ios/Sincilicon.png" density="mdpi" />
    <icon src="res/icons/ios/Sincilicon.png" density="hdpi" />
    <icon src="res/icons/ios/Sincilicon.png" density="xhdpi" />
  </platform>
  <platform name="ios">
    <!-- iOS 8.0+ -->
    <!-- iPhone 6 Plus  -->
    <icon src="res/icons/ios/Sincilicon.png" width="180" height="180" />
    <!-- iOS 7.0+ -->
    <!-- iPhone / iPod Touch  -->
    <icon src="res/icons/ios/Sincilicon.png" width="60" height="60" />
    <icon src="res/icons/ios/Sincilicon.png" width="120" height="120" />
    <!-- iPad -->
    <icon src="res/icons/ios/icon-76.png" width="76" height="76" />
    <icon src="res/icons/ios/icon-76-2x.png" width="152" height="152" />
    <!-- iOS 6.1 -->
    <!-- Spotlight Icon -->
    <icon src="res/icons/ios/Sincilicon.png" width="40" height="40" />
    <icon src="res/icons/ios/Sincilicon.png" width="80" height="80" />
    <!-- iPhone / iPod Touch -->
    <icon src="res/icons/ios/Sincilicon.png" width="57" height="57" />
    <icon src="res/icons/ios/Sincilicon.png" width="114" height="114" />
    <!-- iPad -->
    <icon src="res/icons/ios/icon-72.png" width="72" height="72" />
    <icon src="res/icons/ios/icon-72-2x.png" width="144" height="144" />
    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="res/icons/ios/Sincilicon.png" width="29" height="29" />
    <icon src="res/icons/ios/Sincilicon.png" width="58" height="58" />
    <!-- iPad Spotlight and Settings Icon -->
    <icon src="res/icons/ios/icon-50.png" width="50" height="50" />
    <icon src="res/icons/ios/icon-50-2x.png" width="100" height="100" />
  </platform>
  <platform name="windows">
    <icon src="res/icons/windows/Square150x150Logo.scale-100.png" width="150" height="150" />
    <icon src="res/icons/windows/Square150x150Logo.scale-240.png" width="360" height="360" />
    <icon src="res/icons/windows/Square30x30Logo.scale-100.png" width="30" height="30" />
    <icon src="res/icons/windows/Square310x310Logo.scale-100.png" width="310" height="310" />
    <icon src="res/icons/windows/Square44x44Logo.scale-100.png" width="44" height="44" />
    <icon src="res/icons/windows/Square44x44Logo.scale-240.png" width="106" height="106" />
    <icon src="res/icons/windows/Square70x70Logo.scale-100.png" width="70" height="70" />
    <icon src="res/icons/windows/Square71x71Logo.scale-100.png" width="71" height="71" />
    <icon src="res/icons/windows/Square71x71Logo.scale-240.png" width="170" height="170" />
    <icon src="res/icons/windows/StoreLogo.scale-100.png" width="50" height="50" />
    <icon src="res/icons/windows/StoreLogo.scale-240.png" width="120" height="120" />
    <icon src="res/icons/windows/Wide310x150Logo.scale-100.png" width="310" height="150" />
    <icon src="res/icons/windows/Wide310x150Logo.scale-240.png" width="744" height="360" />
  </platform>
  <platform name="wp8">
    <icon src="res/icons/wp8/ApplicationIcon.png" width="99" height="99" />
    <icon src="res/icons/wp8/Background.png" width="159" height="159" />
  </platform>
  <platform name="android">
    <splash src="res/screens/ios/SplashSincil.png" density="land-hdpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="land-ldpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="land-mdpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="land-xhdpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="port-hdpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="port-ldpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="port-mdpi" />
    <splash src="res/screens/ios/SplashSincil.png" density="port-xhdpi" />
  </platform>
  <platform name="ios">
    <splash src="res/screens/ios/SplashSincil.png" width="320" height="480" />
    <splash src="res/screens/ios/SplashSincil.png" width="640" height="960" />
    <splash src="res/screens/ios/SplashSincil.png" width="768" height="1024" />
    <splash src="res/screens/ios/SplashSincil.png" width="1536" height="2048" />
    <splash src="res/screens/ios/SplashSincil.png" width="1024" height="768" />
    <splash src="res/screens/ios/SplashSincil.png" width="2048" height="1536" />
    <splash src="res/screens/ios/SplashSincil.png" width="640" height="1136" />
    <splash src="res/screens/ios/SplashSincil.png" width="750" height="1334" />
    <splash src="res/screens/ios/SplashSincil.png" width="1242" height="2208" />
    <splash src="res/screens/ios/SplashSincil.png" width="2208" height="1242" />
  </platform>
  <platform name="windows">
    <splash src="res/screens/windows/SplashScreen.scale-100.png" width="620" height="300" />
    <splash src="res/screens/windows/SplashScreen.scale-240.png" width="1152" height="1920" />
    <splash src="res/screens/windows/SplashScreenPhone.scale-240.png" width="1152" height="1920" />
  </platform>
  <platform name="wp8">
    <splash src="res/screens/wp8/SplashScreenImage.png" width="768" height="1280" />
  </platform>
  <preference name="Fullscreen" value="True" />
  <preference name="target-device" value="handset" />
  <preference name="deployment-target" value="" />
  <preference name="BackupWebStorage" value="cloud" />
  <preference name="SuppressesIncrementalRendering" value="False" />
  <plugin name="cordova-plugin-camera" version="2.4.1" />
</widget>